{
  "functions": {
    "app/api/**/*.ts": {
      "runtime": "nodejs18.x",
      "memory": 1024,
      "maxDuration": 60
    }
  },
  "routes": [
    {
      "src": "/api/projects/(?<projectId>[^/]+)/download-folder",
      "dest": "/api/projects/$projectId/download-folder",
      "continue": true
    }
  ],
  "buildCommand": "npm run build",
  "regions": ["iad1"]
} 